     margin: 5px !important; 
    padding: 5px !important;
}

.event_list_table_class .ui-widget-header{
    background-color: rgba(52, 25, 25, 0.64);
    background-image: url('') !important;
    color: #9DCF38;
}
 
.event_list_table_class .ui-state-default{
    background-color: rgba(52, 25, 25, 0.64) !important;
}

.ui-timepicker-div{
    background-color: rgba(52, 25, 25, 0.64);
}

.ui-timepicker-div .ui-widget-content{
    color: #9DCF38;
    background: rgba(63, 54, 54, 0.94);
    border: 1px solid #a8a8a8;
}

.edit_event_button{
    -webkit-box-shadow:rgba(0,0,0,0.2) 0 1px 0 0 !important;
    -moz-box-shadow:rgba(0,0,0,0.2) 0 1px 0 0 !important;
