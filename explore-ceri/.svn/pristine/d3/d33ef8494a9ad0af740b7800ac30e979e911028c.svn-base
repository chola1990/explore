    margin: 5px !important; 
    padding: 5px !important;
}



#div_container_public_calendar{
    background-color: rgba(103, 55, 112, 0.76);
    color: #ffffff;
    width: 62%;
    float: left; 
    height: 467px !important;
    margin: 5px !important; 
    padding: 5px !important;
    overflow-y: auto;
}

